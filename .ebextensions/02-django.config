container_commands:
  01_migrate:
    command: "django-admin.py migrate"
    leader_only: true
  02_compilemessages:
    command: "django-admin.py compilemessages"
  05_seed:
    command: "django-admin.py seed_users --number 150 && django-admin seed_rooms --number 150 && django-admin seed_facilities --number 10 && django-admin seed_amenities --number 10"
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config/wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings